.van-tabs__line {
  background-color: #647bfe !important;
}
  .coupons-list{
    width:100%;
    margin: 0 auto;
  }
  .coupons-item{
    color:#fff;
    font-size:30rpx;
    background:linear-gradient(to right,#eb7073,#f4948a);
    margin-top:20px;
    /* padding-top:10px; */
    border-radius:5px;
    position:relative;
    height: 100%;
    width: 100%;
    max-height: 100%;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    -moz-background-size: 100% 100%;
    overflow: hidden;

  }
  .coupons-item.gray {
    color:#fff;
    font-size:30rpx;
    background:linear-gradient(to right,#aaaaaa,#999999);
    margin-top:20px;
    padding-top:10px;
    border-radius:5px;
    position:relative;
  }
  .coupons-item-box{
    width:100%;
   
  }
  .money-left{
    width: 24%;
    font-size: 48rpx;
    float: left;
    text-align: center;
    height: 70px;
    line-height: 70px;
    color: #F6F6E6;

  }
  .money-left text{
    font-size:28rpx;
    padding-right:6px;
  }
  .money-right{
    float:left;
    margin-top:2%;
  }
  .money-name{
    font-size:30rpx;
    padding-bottom:5px;
  }
  .money-hold{
    font-size:28rpx;
    color:#fff;
    margin-bottom: 5rpx;
  }
  .money-data{
    font-size: 18rpx;
    color:#E91240;
    background: #fff;
    padding:0 6rpx;
  }
  .money-remark{
    font-size: 11px;
  }
  .icon-share{
    font-size: 20px;
  }
  .money-remark.mt-5{
    margin-top: 5rpx;
  }
  .money-data.gray{
    color:#666;
  }
  .money-home{
    float: right;
    color: #FD1731;
    font-size: 32rpx;
    width: 75rpx;
    padding: 8px 10px;
    border-radius: 3px;
    margin-right: 5px;
    display: flex;
    flex-direction: column;
    align-items: center;

  }
  .money-home.gray{
    color:#666;
   
  }
  .gray .money-home{
    color:#333;
    
  }

  .free-txt {
    position: relative;
    display: inline-block;
    font-size: 48rpx!important;
    height: 96rpx;
    line-height: 96rpx;
    width: 96rpx;
    border-radius: 100%;
    border: 2px solid #fff;
    padding: 0!important;
   
    text-align: center;
  }

  .personal-header{
    background: #1e6ce8;
    padding: 5% 0 10%;
    margin-bottom: 16%;
    border-bottom-left-radius: 25rpx;
    border-bottom-right-radius: 25rpx;
}
.personal-wrapper{
    margin: 0 16px ;
}
.personal-wrapper .weui-cell{
  border-radius: 15px;
  background: #f9f9f9;
  margin: 10px 0;
  border: 1px solid #efefef;
}
.personal-wrapper .img{
    position: relative;
    margin-right: 10px;
    border-radius:50% 
}
.personal-wrapper .img .image{
    width: 64px;
    height: 64px; 
    display: block;
}
.personal-wrapper .infor .title{
    font-size: 14px;
    color: #333333;
    
}    
.personal-wrapper .infor .subtext{
    font-size: 12px;
    color: #AAAAAA;
    
} 