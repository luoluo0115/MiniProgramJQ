/* pages/invoice/calculateTax/calculateTax.wxss */
page{
    background: #fff
  }
  .van-tabs__line {
    background: #1e6ce8!important;
  }
  .left{
    float: left
  }
  .tax-btn-cell{
    padding: 10px 15px
  
  }
  .tax-btn{
   width: 100%;
    background: #1e6ce8!important;
    color: #fff!important;
  
  
  }
  .button-hover{
    background: #7966FE!important;
  }
  .tax-wrapper{
    background: #f9f9f9;
    padding-top:10px;
    border-top:1px solid #eee ;
    margin-bottom: 25px;
  }

  .title {
    margin-right: 5px;
    display: inline-block;
    vertical-align: middle;
  }
  .title.title2{
    margin-left: 15px;
  }
  /*提示信息*/
.notice-info{
    line-height: 26px;
    background: #fff;
    color: #999999;
    font-size: 10px;
    border-top: 1px solid #eee;
    padding: 0 10px;
    
}
.notice-info-bg{
  background: #f5f5f5;
  padding: 0 14rpx;
}
/*表格*/
.table {
  border: 1px solid #eee;
  border-right: 0;
  border-bottom: 0;
  width: 100%;
  margin-top: 20px;
  }
  
  .tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
  }
  
  .th, .td {
  padding: 5px;
  border-bottom: 1px solid #eee;
  border-right: 1px solid #eee;
  text-align: center;
  width: 100%;
  font-size:12px;
  }
  
  .th {
  font-weight: 400;
  background-color: #f9f9f9;
  }
  .th1{
    width:9%;
    
  }
  .th2{
    width:55%;
  }
  .th3{
    width:15%;
  }
  .th4{
    width:21%;
  }
  .rowColor{
    background-color: #f9f9f9;
  }
  /*rule-wrapper*/
  .rule-wrapper .h4{
    border-left: #1e6ce8 2px solid ;
    margin: 20rpx 0;
    font-size: 16px;
    color: #333;
    line-height: 28px;
    line-height: 1;
    text-indent: 10rpx;


  }
  .rule-wrapper .p{
    font-size: 12px;
    color: #666;
    line-height: 24px;
    margin: 10rpx 0;

  }
  .rule-wrapper .p text{
    font-size: 14px;
    color: #333;
  }
  .rule-wrapper .p.gray{
    color: #ddd;
    text-align: center;
  }