/* pages/product/pay-order/pay-order.wxss */
.container {
    margin-bottom: 120rpx;
    padding:0rpx;
    background-color: #f8f8f8;
        
  }
 
  .header-container {
    background-color: #fff;
  }
  
 
  
  form {
    width: 100%;
  }
  
  .goods-list {
    width: 100%;
    background-color: #fff;
    margin-bottom: 20rpx;
  }
  
  .goods-list .list-title {
    font-size: 28rpx;
    color: #000;
    padding: 30rpx 0 25rpx 30rpx;
    border-bottom: 1px solid #eee;
  }
  
  .goods-list  .a-goods {
    width: 720rpx;
    display: flex;
    
    padding: 30rpx 30rpx 30rpx 0;
  }
  
  .goods-list  .a-goods .img-box {
    width: 160rpx;
    height: 160rpx;
    overflow: hidden;
    margin-right: 20rpx;
    margin-left: 30rpx;
    background-color: #d8d8d8;
  }
  
  .goods-list .img-box .img {
    width: 160rpx;
    height: 160rpx;
  }
  
  .goods-list  .a-goods .text-box {
    width: 510rpx;
    box-sizing: border-box;
    padding-top: 10rpx;
  }
  
  .a-goods .text-box .arow {
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  
  .a-goods .text-box .arow .goods-name {
    width: 330rpx;
    font-size: 26rpx;
    height: 40rpx;
    color: #000;
    line-height: 1.6;
    overflow: hidden;
  }
  
  .a-goods .text-box .arow01 {
  }
  
  .a-goods .text-box .arow .goods-price {
    font-size: 24rpx;
    color: #0C30DF;
    align-self: flex-start;

  }
  
  .a-goods .text-box .arow .goods-label {
    font-size: 26rpx;
    color: #999;
  }
  
  .a-goods .text-box .arow .goods-num {
    font-size: 26rpx;
    color: #999;
  }
  
  .peisong-way {
    width: 100%;
    background-color: #fff;
    margin-bottom: 20rpx;
  }
  
  .peisong-way .row-box {
    width: 720rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 24rpx 0 24rpx 30rpx;
  }
  
  .peisong-way .row-label {
    font-size: 28rpx;
    color: #000;
  }
  
  .peisong-way .right-text {
    font-size: 28rpx;
    color: #666;
    padding-right: 30rpx;
  }
  
  .peisong-way .liuyan {
    width: 510rpx;
    font-size: 28rpx;
  }
  
  .goods-info {
    width: 100%;
    background-color: #fff;
    padding-bottom: 24rpx;
  }
  
  .goods-info .row-box {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 24rpx 30rpx 12rpx 30rpx;
    font-size: 28rpx;
    color: #000;
  }
  
  .goods-info .row-box .right-text {
    text-align: right;
  }
  
  .jiesuan-box {
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    border-top: 1px solid #eee;
    background-color: #fff;
    z-index: 99;
  }
  .jiesuan-box.iphone {
    height: 130rpx!important;
  }
  .jiesuan-box .to-pay-btn {
    width: 250rpx;
    text-align: center;
    height: 100%;
    line-height: 100rpx;
    background-color: #6a88ff;
    font-size: 32rpx;
    color: #fff;
    border-radius: 0;
    padding: 0;

  }

  
  .jiesuan-box  .left-price {
    display: flex;
    width: 500rpx;
    justify-content: flex-end;
    line-height: 100rpx;
    padding: 0 30rpx 0 0;
    font-size: 28rpx;
    box-sizing: border-box;
  }
  
  .jiesuan-box .total {
    color: #0C30DF;
    text-align: right;
  }
/*优惠券*/
.showCoupon{
  z-index: 9999;
}
.user-coupon{
  width: 100%;
  height: 100rpx;
  background-image:url('data:image/png;base64,');
  color: #fff;
  margin-top: 15rpx;
  height: 100%;
  width: 100%;
  max-height: 100%;
  background-repeat:no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}
  
.coupons-bg,.coupons-image{
  width:100%;
  height:400rpx;
}
.coupons-input{
  background-color:#fff;
  position:absolute;
  width:80%;
  padding:20px 20px 8px 20px;
  top:160px;
  left:5%;
  border-radius:5px;
  border:1px solid #f8b551;
}
.input{
  border:1px solid #f8b551;
  border-radius:3px;
  padding:5px 10px;
  margin-bottom:10px;
  font-size:30rpx;
  width:70%;
  display:inline-block;
}
.primary {
  background:linear-gradient(to right,#eb7073,#f4948a);
  font-size:28rpx;
  border-radius:4px;
  color:#fff;
  text-align:center;
  width:20%;
  display:inline-block;
  float:right;
  height:33px;
  line-height:33px;
}

.coupons-list{
  width:100%;
  margin: 0 auto;
}
.coupons-item{
  color:#fff;
  font-size:30rpx;
  background:linear-gradient(to right,#eb7073,#f4948a);
  margin-top:20px;
  border-radius:5px;
  position:relative;
  height: 100%;
  width: 100%;
  max-height: 100%;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  overflow: hidden;

}
.coupons-item.gray {
  color:#fff;
  font-size:30rpx;
  background:linear-gradient(to right,#aaaaaa,#999999);
  margin-top:20px;
  padding-top:10px;
  border-radius:5px;
  position:relative;
}
.coupons-item-box{
  width:100%;
}
.money-left{
  width: 24%;
  font-size:48rpx;
  float:left;
  text-align:center;
  height:70px;
  line-height:70px;
  color:#F6F6E6
}
.money-left text{
  font-size:28rpx;
  padding-right:6px;
}
.money-right{
  float:left;
  margin-top:2%;
}
.money-name{
  font-size:30rpx;
  padding-bottom:5px;
}
.money-hold{
  font-size:28rpx;
  color:#fff;
}
.money-data{
  font-size: 18rpx;
  color:#E91240;
  background: #fff;
  padding:0 6rpx;
}
.money-remark{
  font-size: 11px;
  margin-top: 5rpx;
}
.money-data.gray{
  color:#666;
}
.money-home{
  float: right;
  color: #FD1731;
  font-size: 32rpx;
  width: 112rpx;
  margin-top:25px;
  border-radius: 3px;
  margin-right: 5px;

  display: flex;
  flex-direction: column;
  align-items: center;

}
.money-home.gray{
  color:#666;
 
}
.gray .money-home{
  color:#333;
  
}
.user-btn {
    text-align: center;
    min-height:auto;
    font-size: 28rpx;
    width: 100%!important;
    background: #677CFB;
    color: #fff;
    border-radius: 54rpx;
    margin-top: 10px;
    font-weight: 400;
}
/*暂不使用优惠券*/
.nouserCoupon{
  width: 100%;
  height: 64rpx;
  line-height: 64rpx;
  border: 1px solid #ddd;
  border-radius: 10rpx;
  margin-top: 20px;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}
.pay-way{
  font-size: 14px;
  padding: 24rpx 0 24rpx 30rpx;

}
.van-cell__title, .van-cell__value {
  color: #1e6ce8;
  font-size: 13px;
}
.van-cell__label {
  margin-top: 3px;
  margin-top: var(--cell-label-margin-top,3px);
  font-size: 10px!important;
  font-size: var(--cell-label-font-size,10px)!important;
  line-height: 10px!important;
  line-height: var(--cell-label-line-height,10px)!important;
  color: #969799;
  color: var(--cell-label-color,#969799);
}

  