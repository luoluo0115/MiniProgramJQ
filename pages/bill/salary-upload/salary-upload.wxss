.page{
  background-color:#fff ;
  padding-bottom: 4%;
}
.bank-title{
  font-size: 14px;
  color: #666;
  height: 64rpx;
  line-height: 64rpx;
}
.fileImg{
  width: 410rpx;
  height: 300rpx;
  border: 2px solid #ddd;
  color: #ddd;
  margin:20rpx auto;
  border-radius: 10rpx;
  overflow: hidden;
}
.image-card{
  width: 100%;
  height: 100%;
  
}
.fileImg .bank-icon{
  font-size: 96rpx;
  color: #ddd;
  height: 300rpx;
  text-align: center;
  line-height: 300rpx;
  display: block;
}
.btn{
  margin: 0 auto;
  width: 410rpx;
}
.up-btn{
  width: 182rpx;
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 15rpx;
  color: #6988FE;
  border: 1px solid rgb(85, 118, 248);
  background: #fff;
  text-align: center;
  font-size: 22rpx;
  display: inline-block;
  margin: 0 10rpx;
}
.choose-btn,.up-btn.blue{
  width: 182rpx;
  height: 64rpx;
  line-height: 64rpx;
  color: #fff;
  background-color: #6988FE;
  border: 1px solid rgb(115, 142, 252);
  border-radius: 15rpx;
  text-align: center;
  font-size: 22rpx;
  display: inline-block;
  margin: 0 10rpx;
}
