page {
  background-color: #f9f9f9;
  padding-bottom: 4%;
}
.bank-title{
  font-size: 14px;
  color: #666;
  height: 64rpx;
  line-height: 64rpx;
}
.fileImg{
  width: 410rpx;
  height: 300rpx;
  border: 2px solid #ddd;
  color: #ddd;
  margin:20rpx auto;
  border-radius: 10rpx;
  overflow: hidden;
}
.image-card{
  width: 100%;
  height: 100%;
  
}
.fileImg .bank-icon{
  font-size: 96rpx;
  color: #ddd;
  height: 300rpx;
  text-align: center;
  line-height: 300rpx;
  display: block;
}
.btn{
  margin: 0 auto;
  width: 410rpx;
}
.up-btn{
  width: 182rpx;
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 15rpx;
  color: #6988FE;
  border: 1px solid rgb(85, 118, 248);
  background: #fff;
  text-align: center;
  font-size: 22rpx;
  display: inline-block;
  margin: 0 10rpx;
}
.choose-btn,.up-btn.blue{
  width: 182rpx;
  height: 64rpx;
  line-height: 64rpx;
  color: #fff;
  background-color: #6988FE;
  border: 1px solid rgb(115, 142, 252);
  border-radius: 15rpx;
  text-align: center;
  font-size: 22rpx;
  display: inline-block;
  margin: 0 10rpx;
}

/*上传*/
.upload-list{
  margin-top: 30rpx;
  font-size: 24rpx;
  color: #333;
}
.upload-list .item{
  padding: 30rpx 0;
}
.upload-list .title{
  border-left:2px solid #1e6ce8;
  padding-left:10px;
  display: inline-block;
  height: 16px;
  line-height: 16px;
  font-size: 28rpx;
  color: #333;
}
.border-bottom{
  border-bottom: 1px solid #eee;
}
.picker-item{
  padding: 0!important;
  font-size: 24rpx!important;
}


.expend-list {
  width: 100%;
  margin-bottom: 20rpx;
}

.expend-item {
  background-color: #fff;
  margin-top: 20rpx;
  overflow: hidden;
}

.expend-list .list-title {
  font-size: 28rpx;
  color: #000;
  padding: 30rpx 0 25rpx 30rpx;
  border-bottom: 1px solid #eee;
}

.expend-list .a-expend {
  display: flex;
  padding: 15rpx 15rpx 15rpx 0;
  border-bottom: 1px solid #eee;

}

.expend-list .a-expend .img-box {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin-right: 20rpx;
  margin-left: 30rpx;
  background-color: #d8d8d8;
}

.expend-list .img-box .img {
  width: 160rpx;
  height: 160rpx;
}

.expend-list .a-expend .text-box {
  width: 510rpx;
  box-sizing: border-box;
  padding-top: 10rpx;
}

.a-expend .text-box .arow {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.a-expend .text-box .arow .label {
  width: 330rpx;
  font-size: 20rpx;
  height: 40rpx;
  /* color: #000; */
  line-height: 1.6;
  overflow: hidden;
}
 

.a-expend .text-box .arow .label-right {
  font-size: 20rpx;
  /* color: #000; */
  align-self: flex-start;

}

 
.title {
  width: 100%;
  display: block;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 20rpx;
  color: #666;
  border-bottom: 1px solid #eee;
}
.title text{
  padding-left: 30rpx;
}