.btm{
  border-top: 1px solid #eee;
  display: flex;
}
.td.btm{
  line-height: 32px;
}
.td.btm{
  border-top:0;
}
.btm .btm-text{
  flex: 1;
}
.btm .btm-text:first-child{
  border-right: 1px solid #eee;
}
.lin{
  display: block;
  line-height: 32px;
}
.lin .btm-text{
  border-top:1px solid #eee;
  line-height: 32px;
  display: block;
}
.lin .btm-text.btm-text0{
  border-top:0;
}